<p id="notice"><%= notice %></p>

<h1>Weight</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Date Weighed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weight.each do |weight| %>
      <tr>
        <td><%= weights.weight %></td>
        <td><%= weights.weight_date %></td>
        <td><%= link_to 'Show', weight %></td>
        <td><%= link_to 'Edit', edit_weights_path(weight) %></td>
        <td><%= link_to 'Destroy', weight, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>



