<p id="notice"><%= notice %></p>

<h1>Weight Data</h1>

<div class="grapes">
  <table>
    <thead>
      <tr>
        <th>Weight</th>
        <th>Date Weighed</th>
        <th colspan="3">Table Actions</th>
      </tr>
    </thead>

    <tbody>
      <% @weight.each do |weights| %>
        <tr>
          <td><%= weights.weight %></td>
          <td><%= weights.weigh_date %></td>
          <td><%= link_to 'Show', weights %></td>
          <td><%= link_to 'Edit', edit_weight_path(weights) %></td>
          <td><%= link_to 'Destroy', weights, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>








<div class="new_item">
  <%= link_to 'New Weight', new_weight_path %>
</div>
