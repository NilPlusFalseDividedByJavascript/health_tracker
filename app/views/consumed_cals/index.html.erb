<p id="notice"><%= notice %></p>

<h1>Listing Consumed Calories</h1>

<div class="table_style_master">
  <div class="apple">


    <table>
      <thead>
        <tr>
          <th>Cal consume</th>
          <th>Consume date</th>
          <th colspan="3">Table Actions</th>
        </tr>
      </thead>

      <tbody>
        <% @consumed_cals.each do |consumed_cal| %>
          <tr>
            <td><%= consumed_cal.cal_consume %></td>
            <td><%= consumed_cal.consume_date %></td>
            <td><%= link_to 'Show', consumed_cal %></td>
            <td><%= link_to 'Edit', edit_consumed_cal_path(consumed_cal) %></td>
            <td><%= link_to 'Destroy', consumed_cal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>
</div>
<br>

<div class="new_item">
<%= link_to 'New Consumed Calorie', new_consumed_cal_path %>
</div>
